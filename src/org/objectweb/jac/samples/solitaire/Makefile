.SUFFIXES: .java .compiled

include $(JAC_ROOT)/Make.rules

CLASSDIR = $(JAC_ROOT)/classes

#
# SUBDIRSBEFORE are compiled before the current directory
#

SUBDIRSBEFORE = 

#
# SUBDIRSAFTER are compiled after the current directory
#

SUBDIRSAFTER  = optimization naming distribution



.java.compiled:
	@echo "Compiling $< ... "
	@$(JAVACCMD) -d $(CLASSDIR) $<
	@touch $@

#
# Source files
#

JAVA = Configuration.java Run.java

#
# Generic production rules
#

default:  $(JAVA:.java=.compiled) sublast
subfirst:
	for i in $(SUBDIRSBEFORE); do (make -C $$i); done
sublast:
	for i in $(SUBDIRSAFTER); do (make -C $$i); done

all:
	@echo "Compiling $(JAVA) ... "
	@$(JAVACCMD) -d $(CLASSDIR) $(JAVA)
	@touch $(JAVA:.java=.compiled)
	for i in $(SUBDIRSAFTER); do (make all -C $$i); done


#
# Clean up rule
#

clean:
	rm -fr *.compiled $(CLASSDIR)/org/objectweb/jac/samples/solitaire/*
	for i in $(SUBDIRSAFTER);  do (make clean -C $$i); done
